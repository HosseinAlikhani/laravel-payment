{
    "name": "d3cr33/payment",
    "description": "Laravel payment",
    "keywords": [
        "d3cr33",
        "laravel",
        "payment"
    ],
    "homepage": "https://github.com/HosseinAlikhani/laravel-payment",
    "license": "MIT",
    "authors": [
        {
            "name": "Hossein Alikhani",
            "email": "hossein.alikhani1374@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/container": "^10.0",
        "illuminate/contracts": "^10.0",
        "illuminate/database": "^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0.4",
        "fakerphp/faker": "^1.21.0",
        "phpunit/phpunit": "^10.0.11"
    },
    "autoload": {
        "psr-4": {
            "D3cr33\\Payment\\": "src",
            "D3cr33\\Payment\\Test\\": "tests"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "D3cr33\\Payment\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "D3cr33\\Payment\\PaymentServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-main": "1.x-dev",
            "dev-master": "1.x-dev"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
